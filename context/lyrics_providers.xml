<?xml version="1.0" encoding="UTF-8"?>
<lyricproviders>
  <provider name="azlyrics.com" charset="utf-8" url="https://www.azlyrics.com/lyrics/{artist}/{title}.html">
    <urlFormat replace=" ._@,;&amp;\/()'&quot;-" with=""/>
