    </extract>
    <exclude>
      <item tag="&lt;B&gt;"/>
      <item begin="&lt;i&gt;[" end="]&lt;/i&gt;"/>
      <item begin="[" end="]"/>
    </exclude>
    <invalidIndicator value="&lt;h1&gt;Welcome to AZLyrics!&lt;/h1&gt;"/>
  </provider>
